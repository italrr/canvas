[import 'lib/mutators.cv'] # bring in mutators
[import 'lib/math.cv']
[import 'lib/io.cv']

[for i from 1 to 50 [
    [if [eq [math:mod i 3] 0]
        [
            [io:out 'fizz ' i '\n']
        ]
        [if [eq [math:mod i 5] 0][
            [io:out 'buzz ' i '\n']
        ]]        
    ]
    [if [eq [math:mod i 15] 0][
        [io:out 'fizzbuzz ' i '\n']
    ]]     
]]